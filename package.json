{
  "name": "boilerplate-vue3-cms",
  "version": "1.0.0",
  "author": "Chain Nguyen <ntrungtn@gmail.com>",
  "private": false,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build:development": "vue-tsc && vite build --mode development",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build:production": "vue-tsc && vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "lint:eslint": "eslint --max-warnings 0  \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier . --write  \"src/**/*.{js,json,tsx,html,md}\"",
    "format": "npm run lint:eslint && npm run lint:prettier",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.15",
    "axios": "^1.2.2",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "sass-loader": "7.3.1",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.47",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "lodash-es": "^4.17.21",
    "prettier": "2.8.1",
    "sass": "^1.57.1",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-slack": "^0.1.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.0.11"
  },
  "engines": {
    "node": ">= 14.18.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
